<h1>Login</h1>
<%= form_for @member_session, as: :member_session, url: { action: "create" } do |f| %>
  <%= render "shared/error_messages", target: @member_session %>
  <%= f.label :cim %><br />
  <%= f.text_field :cim %><br />
  <br />
  <%= f.label :password %><br />
  <%= f.password_field :password %><br />
  <br />
  <%= f.check_box :remember_me %><%= f.label :remember_me %><br />
  <br />
  <%= f.submit "Login" %>
<% end %>